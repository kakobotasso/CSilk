<div id="wrapperAdmin">
	<div id="guardaTelaLogin">
		<div id="telaLogin">
			<h1>Entrar</h1>

			<%= form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f| %>
			  <div><%= f.label :email, "E-mail" %><br />
			  <%= f.email_field :email %></div>

			  <div><%= f.label :password, "Senha" %><br />
			  <%= f.password_field :password %></div>

			  <% if devise_mapping.rememberable? -%>
			    <div style="display:none;"><%# f.check_box :remember_me %> <%# f.label :remember_me, "Mantenha-me conectado" %></div>
			  <% end -%>

			  <div><%= f.submit "Entrar" %></div>
			<% end %>


			<%#= render "links" %>
		</div>
	</div>
</div>
